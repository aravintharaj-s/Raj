#include<iostream>
using namespace std;
int main()
{
    int a[5][5];
    int i,j,x,y,c=0,d=0,e=0;
    for(i=0;i<5;i++)
    {
         for(j=0;j<5;j++)
         {
              cin>>a[i][j];
         }
    }
    for(i=0;i<5;i++)
    {
         for(j=0;j<5;j++)
         {
              if(a[i][j]==1)
              {
                  x=i;y=j;
             }
         }
    }
      c=2-x;
      d=2-y;
      e=abs(c)+abs(d);
     cout<<e;
    return 0;
}
