#include <iostream>
using namespace std;
int main() 
{
     int a[99];
     int x,t;
     cin>>x;
     if(x==0||x==1)
     {
          cout<<"-1";
          exit(0);
     }
     else
     {
          for(int i=0;i<x;i++)
          {
               if(i%2==0)
                    a[i]=i+2;
               else
                    a[i]=i;
          }
     }
     if(x%2==0)
     {
     for(int i=0;i<x;i++)
     {
          cout<<a[i]<<" ";
     }
     }
     else
     {
          
        cout<<"-1 ";  
     }
     
     return 0;
}
