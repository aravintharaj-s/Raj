#include<iostream>
using namespace std;
int main()
{
    char a[50],x;
    int i,j,n,t;
    cin>>n>>t;
    for(i=0;i<n;i++)
    {
         cin>>a[i];
     }
    for(i=0;i<t;i++)
    {
         for(j=0;j<n;j++)
         {
              if(a[j]=='B'&&a[j+1]=='G')
              {
                  
                      a[j]='G';
                      a[j+1]='B';
                      j++;
                       
                 
             }
         }
    }
    for(i=0;i<n;i++)
    {
         cout<<a[i];
     }
      
    return 0;
}
